  <script>
    $(document).ready(function(){
      $("#current-label").click(function(){
        $("#current-plans").show();
        $('#current-label hr').css('visibility', 'visible');
        $("#completed-plans").hide();
        $('#completed-label hr').css('visibility', 'hidden');
      });
      $("#completed-label").click(function(){
        $("#completed-plans").show();
        $('#completed-label hr').css('visibility', 'visible');
        $("#current-plans").hide();
        $('#current-label hr').css('visibility', 'hidden');
      });
    });
    </script>






<section class= "header">
	<div class="container last-time">
		<p class="label">Last Time:</p>
		<!--connect with backend-->
		<h1>Put Plan Name Here</h1>
		<!--connect with backend-->
		<h3>Put Book-name Chapter# Here</h3>
		<div id="resume">
			<!-- Link to reading page -->
			<button href="" type="button" class="btn-lg">Resume Reading</button>
		</div>
	</div>
</section>

<section class="plan-labels">
	<div class="container">
		<div class="row">
			<div id="current-label" class="col-sm-6">
				<p class="label link">Current Plans</p>
				<hr>
			</div>
			<div id="completed-label" class="col-sm-6">
				<p class="label link">Completed Plans</p>
				<hr style="visibility: hidden;">
			</div>
		</div>
	</div>
</section>

<section id="current-plans" class="plans-list">
	<div class="container">
		<div class="row">

	<!-- Link to single plan page (page with stats) -->
  <%for (var planNum in context.plans){%>
    <a class="plan-link" href="/plan/<%=context.plans[planNum]._id%>">
      <div class= "col-sm-12 plan">
        <p><%=context.plans[planNum].planName%></p>
      </div>
      <div class="progress">
        <div class="progress-bar progress-20"></div>
      </div>
    </a>
    <%}%>
      

<!-- link to create new plan page -->
	<a class="create-plan-link" href="/addplan">
			<div class= "col-sm-12 new-plan" >
				<i class="fas fa-plus"></i>
				<p>Create New Plan</p>
			</div>
	</a>

		</div>
	</div>
</section>

<section id="completed-plans" class="plans-list">
	<div class="container">
		<div class="row">

			<a class="plan-link" href="">
				<div class= "col-sm-12 plan">
					<p>Epistles of Paul</p>
				</div>
				<div class="progress">
					<div class="progress-bar progress-complete"></div>
				</div>
			</a>

			<a class="plan-link" href="">
				<div class="col-sm-12 plan">
					<p>Daily Proverbs</p>
				</div>
				<div class="progress">
					<div class="progress-bar progress-complete"></div>
				</div>
			</a>

		</div>
	</div>
</section>













